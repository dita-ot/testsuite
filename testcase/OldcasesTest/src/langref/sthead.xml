<?xml-stylesheet href="../../xsl/dita2html_shell.xsl" type="text/xsl"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "../../dtd/reference.dtd" []>
<reference id="sthead" xml:lang="en-us">
   <title>
      <keyword>sthead</keyword>
   </title>
   <shortdesc>The simpletable header (&lt;sthead&gt;) element contains the table's
header row. The header row is optional in a simple table.</shortdesc>
   <prolog>
      <metadata>
         <keywords>
            <indexterm>sthead</indexterm>
            <indexterm>Table
elements<indexterm>syhead</indexterm>
            </indexterm>
            <indexterm>syhead</indexterm>
         </keywords>
      </metadata>
   </prolog>
   <refbody>
      <refsyn>
         <title>Contained by</title>
         <xref href="simpletable.xml">simpletable</xref>
      </refsyn>
      <refsyn>
         <title>Contains</title>
         <xref href="stentry.xml">stentry</xref>
      </refsyn>
      <section>
         <title>Attributes</title>
         <simpletable relcolwidth="2* 3* 1* 1* 1*" id="sthead_attr">
            <sthead>
               <stentry>Name</stentry>
               <stentry>Description</stentry>
               <stentry>Data Type</stentry>
               <stentry>Default Value</stentry>
               <stentry>Required?</stentry>
            </sthead>
            <strow conref="commonLRdefs.xml#commonLRdefs/univ-atts">
               <stentry/>
               <stentry/>
               <stentry/>
               <stentry/>
               <stentry/>
            </strow>
            <strow conref="commonLRdefs.xml#commonLRdefs/outputclass">
               <stentry/>
               <stentry/>
               <stentry/>
               <stentry/>
               <stentry/>
            </strow>
            <strow conref="commonLRdefs.xml#commonLRdefs/global-atts">
               <stentry/>
               <stentry/>
               <stentry/>
               <stentry/>
               <stentry/>
            </strow>
            <strow conref="commonLRdefs.xml#commonLRdefs/class">
               <stentry/>
               <stentry/>
               <stentry/>
               <stentry/>
               <stentry/>
            </strow>
         </simpletable>
      </section>
      <example>
         <codeblock>&lt;simpletable&gt;
 &lt;sthead&gt;
  &lt;stentry&gt;Type style&lt;/stentry&gt;
  &lt;stentry&gt;Elements used&lt;/stentry&gt;
 &lt;/sthead&gt;
 . . . 
&lt;/simpletable&gt;</codeblock>
      </example>
   </refbody>
</reference>